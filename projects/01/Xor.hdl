/**
 *  Exclusive-or gate: out = !(a == b).
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=nand-ab);
    Or(a=a, b=b, out=or-ab);
    And(a=nand-ab, b=or-ab, out=out);
}
